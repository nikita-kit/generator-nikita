// Color-Variables

// Text-Color
$text-color-01: #fff; // white
$text-color-02: #333; // dark-grey
$text-color-03: #999; // grey
$text-color-04: #eee; // light-grey
$text-color-05: #f9eaa6; // yellow
$text-color-06: #b0ca0a; // green

// Background-Color
$bg-color-01: #fff; // white
$bg-color-02: #333; // dark-grey
$bg-color-03: #999; // grey
$bg-color-04: #eee; // light-grey
$bg-color-05: #f9eaa6; // yellow
$bg-color-06: #b0ca0a; // green

// Link-Color
$link-color-01: #fff; // white
$link-color-02: #333; // dark-grey
$link-color-03: #999; // grey
$link-color-04: #eee; // light-grey
$link-color-05: #f9eaa6; // yellow
$link-color-06: #b0ca0a; // green

// Line-Color
$line-color-01: #fff; // white
$line-color-02: #333; // dark-grey
$line-color-03: #999; // grey
$line-color-04: #eee; // light-grey
$line-color-05: #f9eaa6; // yellow
$line-color-06: #b0ca0a; // green

// Transparent-Color
$transparent: rgba(255, 255, 255, 0); // for IE9 animation
